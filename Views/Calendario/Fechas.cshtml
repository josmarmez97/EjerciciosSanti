@{
    ViewData["Title"] = "Tabla";
}
@model List<EJERCICIOS.Models.Calendario>
<h2>@ViewData["Title"]</h2>

<div>
    <table class="table table-striped">

        <thead>
            <tr>
                <th>Fecha Inicio</th>
                <th>Fecha Fin</th>
                <th>Fecha de Impresion</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.FechaInicio
                    </td>
                    <td>
                        @item.FechaFin
                    </td>
                    <td>
                        @item.DiaImp
                    </td>
                </tr>
            }
        </tbody>

    </table>

    <a href="/Calendario"> <button type="button" class="btn btn-success"> Regresar</button></a>

</div>